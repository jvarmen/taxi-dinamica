@model TaxiDinamica.Web.ViewModels.Partners.PartnersSimpleListViewModel
@{
    this.ViewData["Title"] = "Default";
}

@foreach (var Partner in Model.Partners)
{
    <a asp-area="Manager" asp-controller="Partners" asp-action="Details" asp-route-id="@Partner.Id"
       class="list-group-item list-group-item-action bg-light">@Partner.Name</a>
}
