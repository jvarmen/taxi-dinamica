{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js",
        "dist/jquery.js"
      ]
    },
    {
      "library": "bootstrap@4.6.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "dist/css/bootstrap-datepicker.css",
        "dist/css/bootstrap-datepicker.min.css",
        "dist/js/bootstrap-datepicker.js",
        "dist/js/bootstrap-datepicker.min.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.1",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js",
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "glightbox@3.0.8",
      "destination": "wwwroot/lib/glightbox/",
      "files": [
        "dist/css/glightbox.min.css",
        "dist/js/glightbox.min.js"
      ]
    },
    {
      "library": "aos@2.3.4",
      "destination": "wwwroot/lib/aos/",
      "files": [
        "dist/aos.css",
        "dist/aos.js"
      ]
    },
    {
      "library": "isotope-layout@3.0.6",
      "destination": "wwwroot/lib/isotope-layout/",
      "files": [
        "dist/isotope.pkgd.js",
        "dist/isotope.pkgd.min.js"
      ]
    }
  ]
}